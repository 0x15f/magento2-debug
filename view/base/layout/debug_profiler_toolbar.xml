<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="toolbar"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="ClawRock_Debug::css/toolbar.css"/>
    </head>
    <body>
        <referenceContainer name="debug.toolbar.collectors">
            <block name="debug.toolbar.collectors.request" class="ClawRock\Debug\Block\Profiler\Collector\Request"
                   template="collector/toolbar/request.phtml" cacheable="false"/>
            <block name="debug.toolbar.collectors.memory" class="ClawRock\Debug\Block\Profiler\Collector"
                   template="collector/toolbar/memory.phtml" cacheable="false"/>
            <block name="debug.toolbar.collectors.time" class="ClawRock\Debug\Block\Profiler\Collector\Time"
                   template="collector/toolbar/time.phtml" cacheable="false"/>
            <block name="debug.toolbar.collectors.database" class="ClawRock\Debug\Block\Profiler\Collector\Database"
                   template="collector/toolbar/database.phtml" cacheable="false"/>
            <block name="debug.toolbar.collectors.translation" class="ClawRock\Debug\Block\Profiler\Collector"
                   template="collector/toolbar/translation.phtml" cacheable="false"/>
            <block name="debug.toolbar.collectors.ajax" class="ClawRock\Debug\Block\Profiler\Collector"
                   template="collector/toolbar/ajax.phtml" cacheable="false"/>
            <block name="debug.toolbar.collectors.model" class="ClawRock\Debug\Block\Profiler\Collector"
                   template="collector/toolbar/model.phtml" cacheable="false"/>
            <block name="debug.toolbar.collectors.customer" class="ClawRock\Debug\Block\Profiler\Collector\Customer"
                   template="collector/toolbar/customer.phtml" cacheable="false"/>
            <block name="debug.toolbar.collectors.layout" class="ClawRock\Debug\Block\Profiler\Collector\Layout"
                   template="collector/toolbar/layout.phtml" cacheable="false"/>
            <block name="debug.toolbar.collectors.cache" class="ClawRock\Debug\Block\Profiler\Collector\Cache"
                   template="collector/toolbar/cache.phtml" cacheable="false"/>
            <block name="debug.toolbar.collectors.config" class="ClawRock\Debug\Block\Profiler\Collector\Config"
                   template="collector/toolbar/config.phtml" cacheable="false"/>
            <block name="debug.toolbar.collectors.event" class="ClawRock\Debug\Block\Profiler\Collector"
                   template="collector/toolbar/event.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>
